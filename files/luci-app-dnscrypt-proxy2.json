{
	"luci-app-dnscrypt-proxy2": {
		"description": "Grant access for luci-app-dnscrypt-proxy2",
		"read": {
			"uci": [
				"dnscrypt-proxy2"
			],
			"file": {
				"/etc/dnscrypt-proxy2/*": ["read"],
				"/var/lib/dnscrypt-proxy/*": ["read", "list"],
				"/var/log/dnscrypt-proxy.log": ["read"],
				"/usr/sbin/dnscrypt-proxy": ["exec"],
				"/usr/libexec/dnscrypt-proxy/helper": ["exec"],
				"/etc/init.d/dnscrypt-proxy2": ["exec", "list"]
			},
			"ubus": {
				"file": ["exec", "read", "stat"],
				"luci": ["getFeatures"],
				"service": ["list"],
				"uci": ["get", "state"]
			}
		},
		"write": {
			"uci": [
				"dnscrypt-proxy2"
			],
			"file": {
				"/etc/dnscrypt-proxy2/*": ["write"],
				"/var/lib/dnscrypt-proxy/*": ["write"]
			},
			"ubus": {
				"file": ["write", "exec"],
				"service": ["set", "delete"],
				"uci": ["set", "delete", "commit", "apply", "rollback"]
			}
		}
	}
}
